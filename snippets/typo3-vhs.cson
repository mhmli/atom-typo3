'.text.html':
  'v:asset.prefetch':
    'prefix': 'v:asset.prefetch'
    'body': """<v:asset.prefetch domains="${1:REQUIRED!}" path="${2:Path to file}" external="${3:1}" name="${4:NULL}" overwrite="${5:1}" dependencies="${6:NULL}" group="${7:'fluid'}" debug="${8:1}" standalone="${9:1}" rewrite="${10:1}" fluid="${11:1}" variables="${12:[mixed]}" allowMoveToFooter="${13:1}" movable="${14:1}" trim="${15:1}" namedChunks="${16:1}" protocol="${17:NULL}" protocolSeparator="${18:'//'}" force="${19:1}" content="${20:NULL}">
	${21:<!-- tag content - may be ignored! -->}
</v:asset.prefetch>"""
  'v:asset':
    'prefix': 'v:asset'
    'body': """<v:asset content="${1:NULL}" path="${2:NULL}" external="${3:1}" name="${4:NULL}" overwrite="${5:1}" dependencies="${6:NULL}" group="${7:'fluid'}" debug="${8:1}" standalone="${9:1}" rewrite="${10:1}" fluid="${11:1}" variables="${12:[mixed]}" allowMoveToFooter="${13:1}" movable="${14:1}" trim="${15:1}" namedChunks="${16:1}">
    	${17:<!-- tag content - may be ignored! -->}
</v:asset>"""
